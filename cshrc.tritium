setenv NOMOTD
setenv RSYNC_RSH ssh
setenv CVS_RSH ssh

if ($?INTERACTIVE) then
    complete	ssh	'p/1/(aurora-borealis.dyndns.org ssh-sj1.cisco.com)/'

    cstat "."

    if ($?TERM) then
        if ($TERM == xterm) then
            setenv TERM xterm-color
        endif
    endif
    set dontshowproc

    if (! $?DISPLAY) then
        setenv DISPLAY :0
    endif

    alias em /Applications/Emacs.app/Contents/MacOS/Emacs
endif

cstat "done"
