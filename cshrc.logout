if (`alias rm` == "") then
    set normalias
else
    set rmalias=`alias rm`
    unalias rm
endif

if ($?SHLVL) then
    if ($SHLVL == 1) then
        if ($?SSH2_AGENT_PID) then
            kill ${SSH2_AGENT_PID}
            rm -Rf ${SSH2_AUTH_SOCK}
        else if ($?SSH_AGENT_PID) then
            eval `ssh-agent -k`
        endif
    endif
endif

if ($?normalias) then
    unset normalias
else
    alias rm $rmalias
    unset rmalias
endif
