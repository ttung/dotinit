if (`alias rm` == "") then
    set normalias
else
    set rmalias=`alias rm`
    unalias rm
endif

if ($?ssh_agent_started) then
    if ($?SSH2_AGENT_PID) then
        kill ${SSH2_AGENT_PID}
        rm -Rf ${SSH2_AUTH_SOCK}
    else if ($?SSH_AGENT_PID) then
        eval `ssh-agent -k`
    endif
endif

if ($?normalias) then
    unset normalias
else
    alias rm $rmalias
    unset rmalias
endif
